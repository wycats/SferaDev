{
	"name": "nuki-api-js",
	"version": "0.2.1",
	"description": "Nuki auto-generated type-safe API client",
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee",
		"generate": "rimraf ./src/generated && kubb generate"
	},
	"dependencies": {
		"zod": "catalog:"
	},
	"devDependencies": {
		"@kubb/cli": "catalog:",
		"@kubb/core": "catalog:",
		"@modelcontextprotocol/sdk": "catalog:",
		"@sferadev/openapi-utils": "workspace:*",
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"rimraf": "catalog:",
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"@modelcontextprotocol/sdk": "^1.15.1"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/sdk": {
			"optional": true
		}
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./mcp": {
			"import": {
				"types": "./dist/mcp.d.mts",
				"default": "./dist/mcp.mjs"
			},
			"require": {
				"types": "./dist/mcp.d.cts",
				"default": "./dist/mcp.cjs"
			}
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
