{
	"name": "nuki-api-js",
	"version": "0.3.0",
	"description": "Nuki auto-generated type-safe API client",
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee",
		"generate": "openapi-codegen api"
	},
	"devDependencies": {
		"@openapi-codegen/cli": "catalog:",
		"@openapi-codegen/typescript": "catalog:",
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"case": "catalog:",
		"openapi3-ts": "catalog:",
		"ts-morph": "catalog:",
		"typescript": "catalog:"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
