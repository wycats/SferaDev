{
	"name": "rollup-plugin-import-cdn",
	"version": "0.3.0",
	"description": "Import and transform modules from a CDN to rollup",
	"type": "module",
	"sideEffects": false,
	"license": "ISC",
	"author": "SferaDev",
	"repository": {
		"type": "git",
		"url": "https://github.com/SferaDev/SferaDev.git",
		"directory": "packages/rollup-plugin-import-cdn"
	},
	"keywords": [
		"rollup",
		"plugin",
		"cdn",
		"esm",
		"import",
		"skypack",
		"tree-shaking"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "bunchee",
		"dev": "bunchee --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"tsc": "tsc --noEmit",
		"is-tree-shakable": "is-tree-shakable",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"dependencies": {
		"es-module-lexer": "catalog:"
	},
	"peerDependencies": {
		"rollup": "^3.0.0 || ^4.0.0"
	},
	"devDependencies": {
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"rollup": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
