{
	"name": "@sferadev/openapi-utils",
	"version": "0.0.1",
	"description": "Utilities for OpenAPI clients",
	"private": true,
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"dependencies": {
		"@kubb/core": "catalog:",
		"@kubb/oas": "catalog:",
		"@kubb/plugin-client": "catalog:",
		"@kubb/plugin-mcp": "catalog:",
		"@kubb/plugin-oas": "catalog:",
		"@kubb/plugin-ts": "catalog:",
		"@kubb/plugin-zod": "catalog:",
		"@kubb/react-fabric": "catalog:",
		"case": "catalog:"
	},
	"devDependencies": {
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"bunchee": "catalog:",
		"effect": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	}
