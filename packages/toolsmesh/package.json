{
	"name": "toolsmesh",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SferaDev/SferaDev.git",
		"directory": "packages/toolsmesh"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "bunchee",
		"dev": "bunchee --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"tsc": "tsc --noEmit",
		"check:tree-shake": "is-tree-shakable",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"dependencies": {
		"@ai-sdk/provider": "catalog:",
		"@ai-sdk/provider-utils": "catalog:",
		"just-bash": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"ai": "catalog:",
		"bunchee": "catalog:",
		"is-tree-shakable": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"peerDependencies": {
		"@vercel/sandbox": ">=0.1.0",
		"ai": ">=6.0.0"
	},
	"peerDependenciesMeta": {
		"@vercel/sandbox": {
			"optional": true
		}
	}
}
